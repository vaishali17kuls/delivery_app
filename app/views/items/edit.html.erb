<h1>Editing Item</h1>
<% if @item.errors.any? %>
<h2>The following errors prevented the item from getting created</h2>
<ul>
 <% @item.errors.full_messages.each do |msg| %>
  <li><%= msg %></li>
 <% end %>
</ul>
<% end %>
<%= render 'form' %>

<%= link_to 'Show', @item %> |
<%= link_to 'Back', items_path %>
